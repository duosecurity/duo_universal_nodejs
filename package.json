{
  "name": "@duosecurity/duo_universal",
  "version": "2.1.0",
  "description": "Node.js implementation of the Duo Universal SDK.",
  "keywords": [
    "duo",
    "duo security"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duosecurity/duo_universal_nodejs.git"
  },
  "homepage": "https://github.com/duosecurity/duo_universal_nodejs#readme",
  "bugs": {
    "url": "https://github.com/duosecurity/duo_universal_nodejs/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "build_for_example": "tsup --env.NODE_ENV production --outDir example/node_modules/@duosecurity/duo_universal",
    "build": "tsup --env.NODE_ENV production",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src --fix",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "overrides": {
    "glob-parent": "^6.0.2"
  }
}
